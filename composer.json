{
    "name": "eighty8/stringy",
    "description": "A string manipulation library with multibyte support",
    "keywords": [
        "multibyte", "string", "manipulation", "utility", "methods", "utf-8",
        "helpers", "utils", "utf"
    ],
    "homepage": "https://github.com/eighty8/Stringy",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "symfony/polyfill-mbstring": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "support": {
        "issues": "https://github.com/eighty8/Stringy/issues",
        "source": "https://github.com/eighty8/Stringy"
    },
    "autoload": {
        "psr-4": { "Stringy\\": "src/" },
        "files": ["src/Create.php"]
    },
    "autoload-dev": {
        "classmap": [ "tests" ]
    }
}
